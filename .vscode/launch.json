	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/influxdb/main.go",
			"args": [
				"-writeClients",
				"1",
				"-dataDir",
				"../testdata_100M",
				"-numOfFiles",
				"10",
				"-rowsPerRequest",
				"2000",
				"-replicaN",
				"3",
				"-consistency",
				"all"
			]
		}
	]
}